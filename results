    \u062F\u06A9\u062A\u0631 \u0622\u0646\u0644\u0627\u06CC\u0646 \u062A\u0648\u0627\u0646\u0628\u062E\u0634\u06CC \n        \n         \n     \n\n      \u062F\u06A9\u062A\u0631 \u0622\u0646\u0644\u0627\u06CC\u0646 \u0627\u0632\u062F\u0648\u0627\u062C \n        \n         \n     \n\n      \u062F\u06A9\u062A\u0631 \u0622\u0646\u0644\u0627\u06CC\u0646 \u062E\u0627\u0646\u0648\u0627\u062F\u0647 \n        \n         \n     \n\n      \u062F\u06A9\u062A\u0631 \u0622\u0646\u0644\u0627\u06CC\u0646 \u0637\u0628 \u0627\u0648\u0631\u0698\u0627\u0646\u0633 \n        \n         \n     \n\n      \u062F\u06A9\u062A\u0631 \u0622\u0646\u0644\u0627\u06CC\u0646 \u067E\u0627\u0631\u06A9\u06CC\u0646\u0633\u0648\u0646 \n        \n         \n     \n\n      \u062F\u06A9\u062A\u0631 \u0622\u0646\u0644\u0627\u06CC\u0646 \u0628\u06CC\u0647\u0648\u0634\u06CC \u0648 \u0645\u0631\u0627\u0642\u0628\u062A \u0647\u0627\u06CC \u0648\u06CC\u0698\u0647 \n        \n         \n     \n\n    \n\n   \n\n  \n\n  \n   \n    \n     \u0645\u0631\u0627\u06A9\u0632 \u062F\u0631\u0645\u0627\u0646\u06CC \u0628\u0631\u062A\u0631 \u067E\u0630\u06CC\u0631\u063424\n\n    \n\n    \n      \u0628\u06CC\u0645\u0627\u0631\u0633\u062A\u0627\u0646 \u0646\u06CC\u06A9\u0627\u0646 \n        \n          \n     \n\n      \u0628\u06CC\u0645\u0627\u0631\u0633\u062A\u0627\u0646 \u0631\u0627\u0632\u06CC \n        \n          \n     \n\n      \u0628\u06CC\u0645\u0627\u0631\u0633\u062A\u0627\u0646 \u0631\u0633\u0648\u0644 \u0627\u06A9\u0631\u0645 \n        \n          \n     \n\n      \u0628\u06CC\u0645\u0627\u0631\u0633\u062A\u0627\u0646 \u0627\u0645\u0627\u0645 \u0631\u0636\u0627 \n        \n          \n     \n\n      \u0628\u06CC\u0645\u0627\u0631\u0633\u062A\u0627\u0646 \u0641\u0627\u0631\u0627\u0628\u06CC \n        \n          \n     \n\n      \u0646\u0648\u0628\u062A \u062F\u0647\u06CC \u062A\u0633\u062A \u06A9\u0631\u0648\u0646\u0627 \u0628\u0646\u062F\u0631\u0639\u0628\u0627\u0633 \n        \n          \n     \n\n      \u0628\u